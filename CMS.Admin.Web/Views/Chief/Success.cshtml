@{
    ViewData["Title"] = "Sayaç | Kayıt Durumu";
}
<div id="mapView" class="min"><div class="mapPlaceholder"><span class="fa fa-spin fa-spinner"></span> Loading map...</div></div>
<div id="content" class="max">
    <div class="tables">
        <h4>Kayıt Durumu</h4>
        <div class="row">
            <div class="col-xs-12">
                <div class="panel panel-default">
                    <div class="panel-heading">Başarılı</div>
                    <div class="panel-body">
                        <div class="form-group pb-30">
                            <div class="col-sm-12">
                                <h3>Mükemmel! İşleminiz başarılı bir şekilde gerçekleşmiştir.</h3>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-12">
                                <a asp-action="Create" asp-controller="Chief" class="btn btn-md btn-gray">Geri Dön</a>
                                <a class="btn btn-md btn-green pull-right">Tüm Kayıtları Gör</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="clearfix"></div>